msgid "SEE ALSO"
msgstr "SE ÄVEN"

# type: Plain text
#: ../../../man/po4a/../C/deb-control.5:207
msgid "B<deb>(5), B<dpkg>(1), B<dpkg-deb>(1)."
msgstr "B<deb>(5), B<dpkg>(8), B<dpkg-deb>(1)."
