#, no-wrap
msgid "dpkg"
msgstr "dpkg"

# type: TH
#: ../../../man/po4a/../C/dpkg.1:1
#, no-wrap
msgid "2006-04-09"
msgstr "09-04-2006"

# type: TH
#: ../../../man/po4a/../C/dpkg.1:1
#, no-wrap
msgid "Debian Project"
msgstr "Projet Debian"

# type: TH
