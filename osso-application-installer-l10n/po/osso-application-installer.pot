# Information note shown, when trying to download from a source with a not trusted certificate and if this certificate has an unsupported version.
msgid "ai_ni_unsupported_certificate_version"
msgstr "Certificate version not supported.\nConnection will be refused."

# Information note shown, when when trying to download from a source with a certificate and there is some other error in the certificate than unsupported certificate algorithm or version that will prevent handling the certificate.
msgid "ai_ni_secure_connection_error"
msgstr "Unknown certificate error.\nConnection will be refused."

# Updating the list of packages fails because:
# The server is not available
# The package list is not available
# Verifying the signature fails -> the package list is considered as corrupted
msgid "ai_ni_update_list_not_successful"
msgstr "Unable to refresh list.\nLast refreshed list is shown."

# Downloading the package fails (eg. connection is lost)
msgid "ai_ni_error_download_failed"
msgstr "Download failed"

# The file is not found from the catalogue (eg. the list of packages was old)
msgid "ai_ni_error_download_missing"
msgstr "Unable to download.\nApplication package not found."

# Dependencies cannot be satisfied when installing from file or from catalogue
# 
# Buttons: Details & Close
msgid "ai_ni_error_install_missing"
msgstr "Unable to install.\nSome application packages required for the installation are missing."

# Dependencies cannot be satisfied when updating from file or from catalogue
# 
# Buttons: Details & Close
msgid "ai_ni_error_update_missing"
msgstr "Unable to update.\nSome application packages required for the update are missing."

# The maintainer script fails during install and the package gets broken
msgid "ai_ni_error_installation_failed"
msgstr "Unable to install %s"

# The maintainer script fails during update and the package gets broken
msgid "ai_ni_error_update_failed"
msgstr "Unable to update %s"

# The maintainer script fails during uninstall and the package gets broken
msgid "ai_ni_error_uninstallation_failed"
msgstr "Unable to uninstall %s"

# The package is incompatible because it is not a user package, but a system package, or it is otherwise incompatible with the device.
msgid "ai_ni_error_install_incompatible"
msgstr "Unable to install.\nIncompatible application package."

# The package is incompatible because it is not a user package.but a system package, or it is otherwise incompatible with the device.
msgid "ai_ni_error_update_incompatible"
msgstr "Unable to update.\nIncompatible application package."

# The package conflicts with another package and it cannot be installed (eg. another version of the package would be needed to satisfy dependencies)
# Buttons: Details & Close
msgid "ai_ni_error_install_conflict"
msgstr "Unable to install.\nConflicting application packages."

# The package conflicts with another package and it cannot be updated (eg. another version of the package would be needed to satisfy dependencies)
# Buttons: Details & Close
msgid "ai_ni_error_update_conflict"
msgstr "Unable to update.\nConflicting application packages."

# The installation file is corrupted. 
msgid "ai_ni_error_install_corrupted"
msgstr "Unable to install.\nInstallation file corrupted."

# The update file is corrupted. 
msgid "ai_ni_error_update_corrupted"
msgstr "Unable to update.\nUpdate file corrupted."

# The application cannot be uninstalled because some packages are needed by other applications.
# 
# Buttons: Details & Close
msgid "ai_ni_error_uninstall_packagesneeded"
msgstr "Unable to uninstall.\nSome application packages are needed by other applications."

# Button used in some error notifications
msgid "ai_ni_bd_details"
msgstr "Details"

# Button used in some error notifications
msgid "ai_ni_bd_close"
msgstr "Close"

# Application is still running when the user is trying to update or uninstall it Or application is running when the user tries to install eg. a plug-in for it. %s is the name of the application (eg. Audio player)
msgid "ai_ni_error_uninstall_applicationrunning"
msgstr "%s is open.\nClose application and try again."

# Uninstall timed out (system crash etc)
msgid "ai_ni_error_uninstall_timeout"
msgstr "Operation timed out"

# The device is out-of-resources
msgid "ai_ni_not_enough_memory"
msgstr "Not enough memory to perform operation.\nTry again."

# Some error happens that is not covered by other error cases
msgid "ai_ni_operation_failed"
msgstr "Operation failed"

# The package is created for Nokia 770 and it is not compatible with the new OS edition
msgid "ai_ni_error_n770package_incompatible"
msgstr "Unable to install.\n Application package is incompatible with current software."

# The user tries to cancel the operation (eg.installation) at the moment when it cannot be done
msgid "ai_ib_unable_cancel"
msgstr "Unable to cancel.\nPlease wait."

# The user tries to install the same package again
msgid "ai_ni_package_installed"
msgstr "%s already installed."

