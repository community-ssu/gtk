      <match type="string" value="mdat" offset="4"/>
      <match type="string" value="moov" offset="4"/>
    </magic>
    <glob pattern="*.qt"/>
    <glob pattern="*.mov"/>
    <glob pattern="*.moov"/>
    <glob pattern="*.qtvr"/>
  </mime-type>
