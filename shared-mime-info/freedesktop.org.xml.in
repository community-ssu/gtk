    <glob pattern="*.cls"/>
    <glob pattern="*.dtx"/>
    <glob pattern="*.ins"/>
    <magic priority="10">
      <match type="string" value="%" offset="0"/>
    </magic>
    <magic priority="50">
      <match type="string" value="documentclass" offset="1"/>
    </magic>
  </mime-type>
  <mime-type type="text/x-texinfo">
    <sub-class-of type="text/plain"/>
