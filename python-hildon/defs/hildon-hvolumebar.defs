;;
(define-object HVolumebar
  (in-module "Hildon")
  (parent "HildonVolumebar")
  (c-name "HildonHVolumebar")
  (gtype-id "HILDON_TYPE_HVOLUMEBAR")
)

(define-function hildon_hvolumebar_get_type
  (c-name "hildon_hvolumebar_get_type")
  (return-type "GType")
)

(define-function hildon_hvolumebar_new
  (c-name "hildon_hvolumebar_new")
  (is-constructor-of "HildonHvolumebar")
  (return-type "GtkWidget*")
  (properties
    '("can-focus" (optional))
    '("has-mute" (optional))
  )
)


