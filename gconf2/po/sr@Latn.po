#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Upotreba: %s <dir>\n"

#: ../backends/gconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Upotreba: %s <dir>\n"
"  Spaja pozadinske hijararhije sistema datoteka kao što je:\n"
"    dir/%%gconf.xml\n"
"        poddir1/%%gconf.xml\n"
"        poddir2/%%gconf.xml\n"
"  u:\n"
"    dir/%%gconf-tree.xml\n"

